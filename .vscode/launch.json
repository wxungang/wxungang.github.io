{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "当前html",
      "request": "launch",
      "type": "chrome",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      // "userDataDir": "${tmpdir}",    //会自动生成一个${tmpdir}文件名，如果报错可以开启这个选项
      "file": "${file}" //更改属性名为"file"，属性值此时表示此时打开的文件，就不需要手动改文件路径了。
      // "port":8090
    },
    {
      "type": "node", //用来启动js
      "request": "launch",
      "name": "debugJS", //  修改启动的名字
      "skipFiles": ["node_modules/**"],
      // "program": "${workspaceFolder}\\debug.js"  //默认生成的指向文件的地址
      "program": "${file}" //表示此时打开的文件
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "file": "${workspaceFolder}/react/demo/react.html"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080/react/demo/react.html",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
